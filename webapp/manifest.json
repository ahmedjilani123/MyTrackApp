{
  "_version": "1.60.0",
  "sap.app": {
    "id": "aj.sap.myexpenseapp",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.15.5",
      "toolsId": "d133e813-35e9-4eff-9b30-296c0d1bbe70"
    },
    "dataSources": {
       
      "MainDataSalary": {
        "uri": "./model/SalaryM.json",
        "type": "JSON"
      },
      "ContactInfo": {
        "uri": "./model/Contact.json",
        "type": "JSON"
      },
      "AvatarProfileM": {
        "uri": "./model/AvatarList.json",
        "type": "JSON"
      },
      "UserDataMain": {
        "uri": "./model/UserData.json",
        "type": "JSON"
      },
      "MainDataResource": {
        "uri": "./model/Resource.json",
        "type": "JSON"
      },
      "Material": {
        "uri": "./model/MaterialP.json",
        "type": "JSON"
      },
      "Missing": {
        "uri": "./model/MissingG.json",
        "type": "JSON"
      },
      "Pending": {
        "uri": "./model/PendingG.json",
        "type": "JSON"
      },
      "mainService": {
        "uri": "/odata/v2/trackService/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.120.27",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "aj.sap.myexpenseapp.i18n.i18n"
        }
      },
      "LoginInfoM":{
          "type": "sap.ui.model.json.JSONModel"
      },
      "DashboardM":{
          "type": "sap.ui.model.json.JSONModel"
      },
      "SalaryM": {
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "MainDataSalary"
      },
      "ColumnLayout": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "ContactM": {
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "ContactInfo"
      },
      "AvatarProfile": {
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "AvatarProfileM"
      },
      "UserDataM": {
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "UserDataMain"
      },
      "ResourceM": {
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "MainDataResource"
      },
      "MaterialM": {
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "Material"
      },
      "MissingG": {
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "Missing"
      },
      "PendingG": {
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "Pending"
      },
      "mainService": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
            "useBatch":false
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "aj.sap.myexpenseapp.view",
        "async": true,
        "viewPath": "aj.sap.myexpenseapp.view",
        "bypassed": {
          "target": "NotFound"
        }
      },
      "routes": [
        {
          "name": "RouteInitial",
          "pattern": ":?query:",
          "target": [
            "TargetInitial"
          ]
        },
        {
          "name": "RouteDashboard",
          "pattern": "dashboard/{id}",
          "target": [
            "TargetDashboard",
            "Home"
          ]
        },
        {
          "name": "Home",
          "pattern": "dashboard/{id}/Home",
          "target": [
            "Home"
          ]
        },
        {
          "name": "IncomeExpense",
          "pattern": "dashboard/{id}/IncomeEx",
          "target": [
            "IncomeExpense"
          ]
        },
        {
          "name": "AdminSec",
          "pattern": "dashboard/{id}/adminSec",
          "target": [
            "AdminSec"
          ]
        },
        {
          "name": "AnalyticTran",
          "pattern": "dashboard/{id}/transaction/:type:/:year:",
          "target": [
            "AnalyticTran"
          ]
        },
        {
          "name": "TransactDetail",
          "pattern": "dashboard/{id}/transactdetail",
          "target": [
            "TransactDetail"
          ]
        },
        {
          "name": "BasicInfo",
          "pattern": "dashboard/{id}/BasicInfo",
          "target": [
            "BasicInfo"
          ]
        },
        {
          "name": "Demo",
          "pattern": "dashboard/demo",
          "target": [
            "Demo"
          ]
        }
      ],
      "targets": {
        "TargetInitial": {
          "id": "Initial",
          "name": "Initial"
        },
        "TargetDashboard": {
          "id": "Dashboard",
          "transition": "show",
          "name": "Main.Dashboard"
        },
        "Home": {
          "id": "Home",
          "parent": "TargetDashboard",
          "controlId": "MainApp",
          "controlAggregation": "pages",
          "transition": "show",
          "name": "Main.Section.Home"
        },
        "IncomeExpense": {
          "id": "IncomeEx",
          "parent": "TargetDashboard",
          "controlId": "MainApp",
          "controlAggregation": "pages",
          "transition": "show",
          "name": "Main.Section.IncomeEx"
        },
        "AdminSec": {
          "id": "AdminSec",
          "parent": "TargetDashboard",
          "controlId": "MainApp",
          "controlAggregation": "pages",
          "transition": "show",
          "name": "Main.Section.AdminSec"
        },
        "AnalyticTran": {
          "id": "AnalyticTran",
          "parent": "TargetDashboard",
          "controlId": "MainApp",
          "controlAggregation": "pages",
          "transition": "show",
          "name": "Main.Section.AnalyticTran"
        },
        "TransactDetail": {
          "id": "TransactDetail",
          "parent": "TargetDashboard",
          "controlId": "MainApp",
          "controlAggregation": "pages",
          "transition": "show",
          "name": "Main.Section.TransactDetail"
        },
        "BasicInfo": {
          "id": "BasicInfo",
          "parent": "TargetDashboard",
          "controlId": "MainApp",
          "controlAggregation": "pages",
          "transition": "show",
          "name": "Main.Section.ResourceInfo"
        },
        "Demo": {
          "id": "Demo",
          "parent": "TargetDashboard",
          "controlId": "MainApp",
          "controlAggregation": "pages",
          "transition": "show",
          "name": "Main.Section.Demo"
        },
        "NotFound": {
          "id": "NotFound",
          "controlId": "app",
          "controlAggregation": "pages",
          "name": "NotFound",
          "transition": "show"
        }
      }
    },
    "rootView": {
      "viewName": "aj.sap.myexpenseapp.view.App",
      "type": "XML",
      "id": "App"
    }
  }
}
