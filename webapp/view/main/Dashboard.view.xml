<mvc:View controllerName="aj.sap.myexpenseapp.controller.Dashboard"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m" xmlns:unified="sap.ui.unified" xmlns:tnt="sap.tnt">

<tnt:ToolPage id="ToolpageID">
<tnt:header>
<tnt:ToolHeader class="HeaderCssTt">
<Button icon="sap-icon://menu2" press="onSideNavButtonPress"></Button>
<Button text="My Applications"></Button>
<ToolbarSpacer ></ToolbarSpacer>
 <Button icon="sap-icon://email" press="_EmailMessagePress">
     <customData>
   <BadgeCustomData key="badge" value="2"></BadgeCustomData> 
    </customData>
    </Button>
     <Avatar src="../images/avatar.png" displaySize="XS" displayShape="Circle"></Avatar>
</tnt:ToolHeader>
</tnt:header>
<tnt:sideContent >
<tnt:SideNavigation class="SideNavigationCss" selectedItem="Personal Dashboard" selectedKey="1D">
<tnt:item>
<tnt:NavigationList itemSelect="NavigationPress">
<tnt:NavigationListItem text="Personal Dashboard" key="Home" icon="sap-icon://bbyd-dashboard"></tnt:NavigationListItem>
<tnt:NavigationListItem text="Basic Information" key="BasicInfo" icon="sap-icon://curriculum"></tnt:NavigationListItem>
<tnt:NavigationListItem text="Add Income/Expense" key="IncomeExpense" icon="sap-icon://loan"></tnt:NavigationListItem>
<tnt:NavigationListItem text="Admin Section" key="AdminSec" icon="sap-icon://key-user-settings"></tnt:NavigationListItem>
<tnt:NavigationListItem text="Analytics Transaction" key="AnalyticTran" icon="sap-icon://key-user-settings"></tnt:NavigationListItem>

</tnt:NavigationList>
</tnt:item>
<tnt:fixedItem>
<tnt:NavigationList >
<tnt:NavigationListItem text="Log Out" icon="sap-icon://log" ></tnt:NavigationListItem>
</tnt:NavigationList>
</tnt:fixedItem>
</tnt:SideNavigation>

</tnt:sideContent>
<tnt:mainContents>
<App id="MainApp"></App>
</tnt:mainContents>

</tnt:ToolPage>
    </mvc:View>