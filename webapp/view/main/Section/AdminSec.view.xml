  <ToolbarSpacer ></ToolbarSpacer>
 <Button icon="sap-icon://add" text="User"></Button>
  </OverflowToolbar>
  </headerToolbar>
  <Table >
  <items>
  </items>
  <columns>
  <Column hAlign="Center">
  <header>
  <Label text="Name"></Label>
  </header>
  </Column>
    <Column hAlign="Center">
  <header>
  <Label text="Role"></Label>
  </header>
  </Column>
    <Column hAlign="Center">
  <header>
  <Label text="Status"></Label>
  </header>
  </Column>
     <Column hAlign="Center">
  <header>
  <Label text="Actions"></Label>
  </header>
  </Column>
  </columns>
  </Table>
  </Panel>
   <Panel headerText="Application" class="sapUiSmallMarginTop">
  <headerToolbar>
  <OverflowToolbar >
    <Title text="Application Settings"></Title>
 

  </OverflowToolbar>
  </headerToolbar>
 
  </Panel>
  </Page>
</mvc:View>