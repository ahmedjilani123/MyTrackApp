  <OverflowToolbar >
    <Title text="Manage Categories"></Title>
 
  <ToolbarSpacer ></ToolbarSpacer>
 <Button icon="sap-icon://add" text="Add"></Button>
  </OverflowToolbar>
  </headerToolbar>
  <Table >
  <items>
  </items>
  <columns>
  <Column hAlign="Center">
  <header>
  <Label text="Category"></Label>
  </header>
  </Column>
    <Column hAlign="Center">
  <header>
  <Label text="Type(Income/Expense)"></Label>
  </header>
  </Column>
    <Column hAlign="Center">
  <header>
  <Label text="Actions"></Label>
  </header>
  </Column>
  </columns>
  </Table>
  </Panel>
   <Panel headerText="User Mamage" class="sapUiSmallMarginTop">
