   <core:Icon src="sap-icon://course-program" class="sapUiSmallMargin" size="2.25rem" color="#3b90fe"></core:Icon>
    <VBox justifyContent="Center" alignItems="Center" class="sapUiSmallMarginEnd">
    <Title text="Gatepass " class="sapAddInvestCss" textAlign="Center"></Title>
   <Title text="88140" level="H4" class="sapAddInvestCss" titleStyle="H4" textAlign="Center"></Title>
