    </f:GridList>
    <layout:Grid   defaultSpan="XL6 L6 M6 S12" width="100%">
    <f:Card width="100%" height="415px">
    <f:layoutData>
    <layout:GridData span="XL6 L6 M6 S12"></layout:GridData>
    </f:layoutData>
<f:content>
<commons:ChartContainer contentChange="showTableChartHandler" title="Pending Gatepass"  width="100%" id="demofirstPlantC_Id" showFullScreen="true" showLegendButton="false" showLegend="true" showZoom="false">

<commons:content>
<commons:ChartContainerContent icon="sap-icon://vertical-stacked-chart">
<commons:content>
<controls:VizFrame  height="365px" vizType="bar" id="demo1VizFrameID">
<controls:dataset >
<data:FlattenedDataset data="{MaterialM>/}">
<data:dimensions>
  <data:DimensionDefinition name="Project Name" value="{MaterialM>ProjectName}"></data:DimensionDefinition>
</data:dimensions>
<data:measures>
    <data:MeasureDefinition name="Number" value="{MaterialM>Number}"></data:MeasureDefinition>
        <data:MeasureDefinition name="Pending Count" value="{MaterialM>PendingCount}"></data:MeasureDefinition>
                                             
</data:measures>
</data:FlattenedDataset>
</controls:dataset> <!-- <ScrollContainer vertical="true" horizontal="false"> -->
<controls:feeds>
  <feeds:FeedItem values="Number,Pending Count" uid="valueAxis" type="Measure"></feeds:FeedItem>
<feeds:FeedItem values="Project Name" uid="categoryAxis" type="Dimension"></feeds:FeedItem>
</controls:feeds>

</controls:VizFrame>
</commons:content>
</commons:ChartContainerContent>


 <commons:ChartContainerContent icon="sap-icon://table-view" >
                                    <commons:content>
                                    <ScrollContainer vertical="true" horizontal="false">
<List items="{MaterialM>/}">
<StandardListItem info="{MaterialM>PendingCount}" infoStateInverted="true" infoState="{path:'MaterialM>PendingCount',formatter:'.MaterialFormat'}" title="{MaterialM>Number} {MaterialM>ProjectName}"></StandardListItem>
</List>
                                    </ScrollContainer>
                                    </commons:content>
                                </commons:ChartContainerContent>
</commons:content>
</commons:ChartContainer>
</f:content>
</f:Card>
  <layout:Grid defaultSpan="XL12 L12 M12 S12" vSpacing="1" hSpacing="0">
    <f:Card width="100%" height="200px">
    <f:layoutData>
    <layout:GridData span="XL12 L12 M12 S12"></layout:GridData>
    </f:layoutData>
<f:content>
<commons:ChartContainer title="Missing Gatepass"  width="100%" id="dmemofirstPlantC_Id" showFullScreen="true" showLegendButton="false" showLegend="true" showZoom="false">

<commons:content>
<commons:ChartContainerContent icon="sap-icon://vertical-stacked-chart">
<commons:content>
<controls:VizFrame height="150px" vizType="line" id="demo2VizFrameID">
<controls:dataset >
<data:FlattenedDataset data="{MissingG>/}">
<data:dimensions>
  <data:DimensionDefinition name="Name" value="{MissingG>name}"></data:DimensionDefinition>
</data:dimensions>
<data:measures>
    <data:MeasureDefinition name="Number" value="{MissingG>value}"></data:MeasureDefinition>
                                                
</data:measures>
</data:FlattenedDataset>
</controls:dataset>
<controls:feeds>
  <feeds:FeedItem values="Number" uid="valueAxis" type="Measure"></feeds:FeedItem>
<feeds:FeedItem values="Name" uid="categoryAxis" type="Dimension"></feeds:FeedItem>
</controls:feeds>
</controls:VizFrame>
</commons:content>
</commons:ChartContainerContent>
 <commons:ChartContainerContent icon="sap-icon://table-view" >
                                    <commons:content>
                                    <ScrollContainer vertical="true" horizontal="false">
<List items="{MissingG>/}">
<StandardListItem   icon="{MissingG>icon}" info="{MissingG>value}" title="{MissingG>name}"></StandardListItem>
</List>
                                    </ScrollContainer>
                                    </commons:content>
                                </commons:ChartContainerContent>
</commons:content>
</commons:ChartContainer>
</f:content>
</f:Card>
 <f:Card width="100%" height="200px">
    <f:layoutData>
    <layout:GridData span="XL12 L12 M12 S12"></layout:GridData>
    </f:layoutData>
<f:content>
<commons:ChartContainer title="Today Gatepass (29-10-2025)"  width="100%" id="dsemofirstPlantC_Id" showFullScreen="true" showLegendButton="false" showLegend="true" showZoom="false">

<commons:content>
<commons:ChartContainerContent icon="sap-icon://vertical-stacked-chart">
<commons:content>
<controls:VizFrame height="150px" vizType="column" id="demo3VizFrameID">
<controls:dataset >
<data:FlattenedDataset data="{PendingG>/}">
<data:dimensions>
  <data:DimensionDefinition name="Name" value="{PendingG>name}"></data:DimensionDefinition>
</data:dimensions>
<data:measures>
    <data:MeasureDefinition name="Number" value="{PendingG>value}"></data:MeasureDefinition>
                                                
</data:measures>
</data:FlattenedDataset>
</controls:dataset>
<controls:feeds>
  <feeds:FeedItem values="Number" uid="valueAxis" type="Measure"></feeds:FeedItem>
<feeds:FeedItem values="Name" uid="categoryAxis" type="Dimension"></feeds:FeedItem>
</controls:feeds>
</controls:VizFrame>
</commons:content>
</commons:ChartContainerContent >
 <commons:ChartContainerContent icon="sap-icon://table-view"  >
                                    <commons:content>
                                    <ScrollContainer   vertical="true" horizontal="false">
<List items="{PendingG>/}" sticky="ColumnHeaders">
<StandardListItem icon="{PendingG>icon}"  info="{PendingG>value}" title="{PendingG>name}"></StandardListItem>
</List>
                                    </ScrollContainer>
                                    </commons:content>
                                </commons:ChartContainerContent>
</commons:content>
</commons:ChartContainer>
</f:content>
</f:Card>
 
  </layout:Grid>
    </layout:Grid>
    </content>
    </Page>
   
</mvc:View>